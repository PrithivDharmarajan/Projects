<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/settings_parent_lay"
    style="@style/LayoutMatch"
    android:background="@color/gray_light"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/settings_header_bg_lay"
        style="@style/LayoutWidMatch_HeiWrap"
        android:layout_height="@dimen/size45"
        android:background="@color/blue">

        <include layout="@layout/ui_general_header" />

    </RelativeLayout>

    <ScrollView style="@style/LayoutMatch">

        <LinearLayout
            style="@style/LayoutWidMatch_HeiWrap"
            android:layout_gravity="center"
            android:orientation="vertical">
            <Button
                android:id="@+id/agent_btn"
                style="@style/ButtonRobotoMedium"
                android:layout_marginBottom="@dimen/size5"
                android:layout_marginEnd="@dimen/size20"
                android:layout_marginStart="@dimen/size20"
                android:layout_marginTop="@dimen/size5"
                android:elevation="0dip"
                android:text="@string/enable_agent"
                android:textColor="@color/white"
                tools:targetApi="lollipop" />
        </LinearLayout>


    </ScrollView>

</LinearLayout>
