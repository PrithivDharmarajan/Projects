                    android:textColor="@color/black"
                    android:textSize="@dimen/text18" />

                <CheckBox
                    android:id="@+id/loft"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/padding10"
                    android:button="@drawable/tick_off" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin1"
                android:layout_marginLeft="@dimen/margin10"
                android:layout_marginRight="@dimen/margin10"
                android:background="@drawable/view_bg" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin50" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/margin50"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="@dimen/padding5"
                    android:gravity="center|left"
                    android:paddingLeft="@dimen/padding5"
                    android:text="@string/tic"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text18" />

                <CheckBox
                    android:id="@+id/tic"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/padding10"
                    android:button="@drawable/tick_off" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin1"
                android:layout_marginLeft="@dimen/margin10"
                android:layout_marginRight="@dimen/margin10"
                android:background="@drawable/view_bg" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin50" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/margin50"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="@dimen/padding5"
                    android:gravity="center|left"
                    android:paddingLeft="@dimen/padding5"
                    android:text="@string/apartment_condo"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text18" />

                <CheckBox
                    android:id="@+id/apartment_condo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/padding10"
                    android:button="@drawable/tick_off" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin1"
                android:layout_marginLeft="@dimen/margin10"
                android:layout_marginRight="@dimen/margin10"
                android:background="@drawable/view_bg" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin50" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/margin50"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="@dimen/padding5"
                    android:gravity="center|left"
                    android:paddingLeft="@dimen/padding5"
                    android:text="@string/mobile_manufactured"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text18" />

                <CheckBox
                    android:id="@+id/mobile_manufactured"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/padding10"
                    android:button="@drawable/tick_off" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin1"
                android:layout_marginLeft="@dimen/margin10"
                android:layout_marginRight="@dimen/margin10"
                android:background="@drawable/view_bg" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin50" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/margin50"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="@dimen/padding5"
                    android:gravity="center|left"
                    android:paddingLeft="@dimen/padding5"
                    android:text="@string/farm_ranch"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text18" />

                <CheckBox
                    android:id="@+id/farm_ranch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/padding10"
                    android:button="@drawable/tick_off" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin1"
                android:layout_marginLeft="@dimen/margin10"
                android:layout_marginRight="@dimen/margin10"
                android:background="@drawable/view_bg" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin50" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/margin50"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="@dimen/padding5"
                    android:gravity="center|left"
                    android:paddingLeft="@dimen/padding5"
                    android:text="@string/lot_land"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text18" />

                <CheckBox
                    android:id="@+id/lot_land"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/padding10"
                    android:button="@drawable/tick_off" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin1"
                android:layout_marginLeft="@dimen/margin10"
                android:layout_marginRight="@dimen/margin10"
                android:background="@drawable/view_bg" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin50" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/margin50"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="@dimen/padding5"
                    android:gravity="center|left"
                    android:paddingLeft="@dimen/padding5"
                    android:text="@string/multi_family"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text18" />

                <CheckBox
                    android:id="@+id/multi_family"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/padding10"
                    android:button="@drawable/tick_off" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin1"
                android:layout_marginLeft="@dimen/margin10"
                android:layout_marginRight="@dimen/margin10"
                android:background="@drawable/view_bg" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin50" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/margin50"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="@dimen/padding5"
                    android:gravity="center|left"
                    android:paddingLeft="@dimen/padding5"
                    android:text="@string/income_investment"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text18" />

                <CheckBox
                    android:id="@+id/income_investment"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/padding10"
                    android:button="@drawable/tick_off" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin1"
                android:layout_marginLeft="@dimen/margin10"
                android:layout_marginRight="@dimen/margin10"
                android:background="@drawable/view_bg" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin50" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/margin50"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="@dimen/padding5"
                    android:gravity="center|left"
                    android:paddingLeft="@dimen/padding5"
                    android:text="@string/houseboat"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text18" />

                <CheckBox
                    android:id="@+id/houseboat"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/padding10"
                    android:button="@drawable/tick_off" />
            </RelativeLayout>
        </LinearLayout>
    </ScrollView>

    <View
        android:id="@+id/view_bottom"
        android:layout_width="match_parent"
        android:layout_height="@dimen/margin1"
        android:layout_above="@+id/bottom_lay"
        android:background="@drawable/view_bg" />

    <LinearLayout
        android:id="@+id/bottom_lay"
        android:layout_width="match_parent"
        android:layout_height="@dimen/margin50"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal"
        android:weightSum="2" >

        <Button
            android:id="@+id/cancel"
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:text="@string/cancel"
            android:textColor="@color/blue" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@drawable/view_bg" />

        <Button
            android:id="@+id/set"
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:text="@string/set"
            android:textColor="@color/blue" />
    </LinearLayout>

</RelativeLayout>