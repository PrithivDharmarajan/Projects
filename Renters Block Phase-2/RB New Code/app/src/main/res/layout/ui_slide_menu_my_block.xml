<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/LayoutWidMatch_HeiWrap"
    android:background="@color/black"
    android:orientation="vertical">

    <TextView
        style="@style/TextHelveticaBold"
        android:background="@color/black"
        android:gravity="start"
        android:paddingBottom="@dimen/size3"
        android:paddingEnd="0dip"
        android:paddingStart="@dimen/size10"
        android:paddingTop="@dimen/size3"
        android:text="@string/my_block"
        android:textAllCaps="true"
        android:textSize="@dimen/text14"
        android:textColor="@color/white" />

    <LinearLayout
        android:id="@+id/listing_lay"
        style="@style/LayoutWidMatch_HeiWrap"
        android:layout_height="@dimen/size45"
        android:background="?android:attr/selectableItemBackground"
        android:paddingStart="@dimen/size7"
        android:paddingEnd="0dip"
        tools:ignore="UseCompoundDrawables">

        <ImageView
            android:layout_width="@dimen/size23"
            android:layout_height="@dimen/size23"
            android:layout_gravity="center"
            android:adjustViewBounds="true"
            android:contentDescription="@string/app_name"
            android:src="@drawable/menu_listing_icon" />

        <TextView
            android:id="@+id/listing_txt"
            style="@style/TextHelveticaLight"
            android:layout_gravity="center"
            android:layout_marginStart="@dimen/size5"
            android:gravity="start"
            android:text="@string/listing"
            android:textColor="@color/white"
            android:textSize="@dimen/text14" />
    </LinearLayout>

    <View
        android:id="@+id/listing_view"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size1"
        android:layout_marginStart="@dimen/size37"
        android:background="@color/black" />

    <LinearLayout
        android:id="@+id/hot_leads_lay"
        style="@style/LayoutWidMatch_HeiWrap"
        android:layout_height="@dimen/size45"
        android:background="?android:attr/selectableItemBackground"
        android:paddingStart="@dimen/size7"
        android:paddingEnd="0dip"
        tools:ignore="UseCompoundDrawables">

        <ImageView
            android:layout_width="@dimen/size23"
            android:layout_height="@dimen/size23"
            android:layout_gravity="center"
            android:adjustViewBounds="true"
            android:contentDescription="@string/app_name"
            android:src="@drawable/menu_hot_leads_icon" />

        <TextView
            style="@style/TextHelveticaLight"
            android:layout_gravity="center"
            android:layout_marginStart="@dimen/size5"
            android:gravity="start"
            android:text="@string/hot_leads"
            android:textColor="@color/white"
            android:textSize="@dimen/text14" />
    </LinearLayout>

    <View
        android:id="@+id/hot_leads_view"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size1"
        android:layout_marginStart="@dimen/size37"
        android:background="@color/black" />

    <LinearLayout
        android:id="@+id/reviews_lay"
        style="@style/LayoutWidMatch_HeiWrap"
        android:layout_height="@dimen/size45"
        android:background="?android:attr/selectableItemBackground"
        android:paddingStart="@dimen/size7"
        android:paddingEnd="0dip"
        tools:ignore="UseCompoundDrawables">

        <ImageView
            android:layout_width="@dimen/size23"
            android:layout_height="@dimen/size23"
            android:layout_gravity="center"
            android:adjustViewBounds="true"
            android:contentDescription="@string/app_name"
            android:src="@drawable/menu_reviews_icon" />

        <TextView
            style="@style/TextHelveticaLight"
            android:layout_gravity="center"
            android:layout_marginStart="@dimen/size5"
            android:gravity="start"
            android:text="@string/reviews"
            android:textColor="@color/white"
            android:textSize="@dimen/text14" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/size1"
        android:layout_marginStart="@dimen/size37"
        android:background="@color/black" />

    <LinearLayout
        android:id="@+id/friends_lay"
        style="@style/LayoutWidMatch_HeiWrap"
        android:layout_height="@dimen/size45"
        android:background="?android:attr/selectableItemBackground"
        android:paddingStart="@dimen/size7"
        android:paddingEnd="0dip"
        tools:ignore="UseCompoundDrawables">

        <ImageView
            android:layout_width="@dimen/size23"
            android:layout_height="@dimen/size23"
            android:layout_gravity="center"
            android:adjustViewBounds="true"
            android:contentDescription="@string/app_name"
            android:src="@drawable/menu_friends_icon" />

        <TextView
            style="@style/TextHelveticaLight"
            android:layout_gravity="center"
            android:layout_marginStart="@dimen/size5"
            android:gravity="start"
            android:text="@string/friends"
            android:textColor="@color/white"
            android:textSize="@dimen/text14" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/size1"
        android:layout_marginStart="@dimen/size37"
        android:background="@color/black" />

    <LinearLayout
        android:id="@+id/scheduling_lay"
        style="@style/LayoutWidMatch_HeiWrap"
        android:layout_height="@dimen/size45"
        android:background="?android:attr/selectableItemBackground"
        android:paddingStart="@dimen/size7"
        android:paddingEnd="0dip"
        tools:ignore="UseCompoundDrawables">

        <ImageView
            android:layout_width="@dimen/size23"
            android:layout_height="@dimen/size23"
            android:layout_gravity="center"
            android:adjustViewBounds="true"
            android:contentDescription="@string/app_name"
            android:src="@drawable/menu_scheduling_icon" />

        <TextView
            style="@style/TextHelveticaLight"
            android:layout_gravity="center"
            android:layout_marginStart="@dimen/size5"
            android:gravity="start"
            android:text="@string/scheduling"
            android:textColor="@color/white"
            android:textSize="@dimen/text14" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/size1"
        android:layout_marginStart="@dimen/size37"
        android:background="@color/black" />

    <LinearLayout
        android:id="@+id/profile_lay"
        style="@style/LayoutWidMatch_HeiWrap"
        android:layout_height="@dimen/size45"
        android:background="?android:attr/selectableItemBackground"
        android:paddingStart="@dimen/size7"
        android:paddingEnd="0dip"
        tools:ignore="UseCompoundDrawables">

        <ImageView
            android:layout_width="@dimen/size23"
            android:layout_height="@dimen/size23"
            android:layout_gravity="center"
            android:padding="@dimen/size1"
            android:adjustViewBounds="true"
            android:contentDescription="@string/app_name"
            android:src="@drawable/menu_profile_icon" />

        <TextView
            style="@style/TextHelveticaLight"
            android:layout_gravity="center"
            android:layout_marginStart="@dimen/size5"
            android:gravity="start"
            android:text="@string/profile"
            android:textColor="@color/white"
            android:textSize="@dimen/text14" />
    </LinearLayout>

</LinearLayout>