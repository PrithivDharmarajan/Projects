<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/LayoutWidMatch_HeiWrap"
    android:orientation="vertical">

    <TextView
        style="@style/TextHelveticaBold"
        android:background="@color/black"
        android:gravity="start"
        android:paddingBottom="@dimen/size3"
        android:paddingEnd="0dip"
        android:paddingStart="@dimen/size10"
        android:paddingTop="@dimen/size3"
        android:text="@string/search"
        android:textAllCaps="true"
        android:textColor="@color/white"
        android:textSize="@dimen/text14" />

    <LinearLayout
        android:id="@+id/exclusives_lay"
        style="@style/LayoutWidMatch_HeiWrap"
        android:layout_height="@dimen/size45"
        android:background="?android:attr/selectableItemBackground"
        android:paddingStart="@dimen/size7"
        android:paddingEnd="0dip"
        tools:ignore="UseCompoundDrawables">

        <ImageView
            android:layout_width="@dimen/size23"
            android:layout_height="@dimen/size23"
            android:layout_gravity="center"
            android:adjustViewBounds="true"
            android:contentDescription="@string/app_name"
            android:src="@drawable/menu_exclusives_icon" />

        <TextView
            style="@style/TextHelveticaLight"
            android:layout_gravity="center"
            android:layout_marginStart="@dimen/size5"
            android:gravity="start"
            android:text="@string/exclusives"
            android:textColor="@color/white"
            android:textSize="@dimen/text14" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/size1"
        android:layout_marginStart="@dimen/size37"
        android:background="@color/black" />

    <LinearLayout
        android:id="@+id/homes_for_rent_lay"
        style="@style/LayoutWidMatch_HeiWrap"
        android:layout_height="@dimen/size45"
        android:background="?android:attr/selectableItemBackground"
        android:paddingStart="@dimen/size7"
        android:paddingEnd="0dip"
        tools:ignore="UseCompoundDrawables">

        <ImageView
            android:layout_width="@dimen/size23"
            android:layout_height="@dimen/size23"
            android:layout_gravity="center"
            android:adjustViewBounds="true"
            android:contentDescription="@string/app_name"
            android:padding="@dimen/size1"
            android:src="@drawable/menu_home_for_rent_icon" />

        <TextView
            style="@style/TextHelveticaLight"
            android:layout_gravity="center"
            android:layout_marginStart="@dimen/size5"
            android:gravity="start"
            android:text="@string/homes_for_rent"
            android:textColor="@color/white"
            android:textSize="@dimen/text14" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/size1"
        android:layout_marginStart="@dimen/size37"
        android:background="@color/black" />

    <LinearLayout
        android:id="@+id/homes_for_sale_lay"
        style="@style/LayoutWidMatch_HeiWrap"
        android:layout_height="@dimen/size45"
        android:background="?android:attr/selectableItemBackground"
        android:paddingStart="@dimen/size7"
        android:paddingEnd="0dip"
        tools:ignore="UseCompoundDrawables">

        <ImageView
            android:layout_width="@dimen/size23"
            android:layout_height="@dimen/size23"
            android:layout_gravity="center"
            android:adjustViewBounds="true"
            android:contentDescription="@string/app_name"
            android:src="@drawable/menu_home_for_sale_icon" />

        <TextView
            style="@style/TextHelveticaLight"
            android:layout_gravity="center"
            android:layout_marginStart="@dimen/size5"
            android:gravity="start"
            android:text="@string/homes_for_sale"
            android:textColor="@color/white"
            android:textSize="@dimen/text14" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/size1"
        android:layout_marginStart="@dimen/size37"
        android:background="@color/black" />

    <LinearLayout
        android:id="@+id/saved_searches_lay"
        style="@style/LayoutWidMatch_HeiWrap"
        android:layout_height="@dimen/size45"
        android:background="?android:attr/selectableItemBackground"
        android:paddingStart="@dimen/size7"
        android:paddingEnd="0dip"
        tools:ignore="UseCompoundDrawables">

        <ImageView
            android:layout_width="@dimen/size23"
            android:layout_height="@dimen/size23"
            android:layout_gravity="center"
            android:adjustViewBounds="true"
            android:contentDescription="@string/app_name"
            android:src="@drawable/menu_saved_searches_icon" />

        <TextView
            style="@style/TextHelveticaLight"
            android:layout_gravity="center"
            android:layout_marginStart="@dimen/size5"
            android:gravity="start"
            android:text="@string/saved_searches"
            android:textColor="@color/white"
            android:textSize="@dimen/text14" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/size1"
        android:layout_marginStart="@dimen/size37"
        android:background="@color/black" />

    <LinearLayout
        android:id="@+id/my_favourites_lay"
        style="@style/LayoutWidMatch_HeiWrap"
        android:layout_height="@dimen/size45"
        android:background="?android:attr/selectableItemBackground"
        android:paddingStart="@dimen/size7"
        android:paddingEnd="0dip"
        tools:ignore="UseCompoundDrawables">

        <ImageView
            android:layout_width="@dimen/size23"
            android:layout_height="@dimen/size23"
            android:layout_gravity="center"
            android:adjustViewBounds="true"
            android:contentDescription="@string/app_name"
            android:src="@drawable/menu_my_favourites_icon" />

        <TextView
            style="@style/TextHelveticaLight"
            android:layout_gravity="center"
            android:layout_marginStart="@dimen/size5"
            android:gravity="start"
            android:text="@string/my_favourites"
            android:textColor="@color/white"
            android:textSize="@dimen/text14" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/size1"
        android:layout_marginStart="@dimen/size37"
        android:background="@color/black" />

    <LinearLayout
        android:id="@+id/alerts_lay"
        style="@style/LayoutWidMatch_HeiWrap"
        android:layout_height="@dimen/size45"
        android:background="?android:attr/selectableItemBackground"
        android:paddingStart="@dimen/size7"
        android:paddingEnd="0dip"
        tools:ignore="UseCompoundDrawables">

        <ImageView
            android:layout_width="@dimen/size23"
            android:layout_height="@dimen/size23"
            android:layout_gravity="center"
            android:adjustViewBounds="true"
            android:contentDescription="@string/app_name"
            android:src="@drawable/menu_alerts_icon" />

        <TextView
            style="@style/TextHelveticaLight"
            android:layout_gravity="center"
            android:layout_marginStart="@dimen/size5"
            android:gravity="start"
            android:text="@string/alerts"
            android:textColor="@color/white"
            android:textSize="@dimen/text14" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/size1"
        android:layout_marginStart="@dimen/size37"
        android:background="@color/black" />

    <LinearLayout
        android:id="@+id/open_houses_lay"
        style="@style/LayoutWidMatch_HeiWrap"
